import re

def lessFive():
    c = 0
    for i in re.findall(r'[0-9]{1,}', input()):
        if int(i)<5:
            c+=1
    print(c)

def unusedChar():
    c = 'AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz'
    for i in re.findall(r'[a-zA-z]',input()):
        c = c.replace(i,'')
    print(c)

def moreFive():
    c = 0
    for i in re.findall(r'\d', input()):
        if int(i)>5:
            c+=1
    print(c)

a = [lessFive,unusedChar,moreFive]
print(a[int(input())](input()))